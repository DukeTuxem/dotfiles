" Color settings
" --------------

" Will likely fallback to 256 color mode if those lines are commented out.
let &t_8f = "\<Esc>[38;2;%lu;%lu;%lum"  " From :help xterm-true-color (but st)
let &t_8b = "\<Esc>[48;2;%lu;%lu;%lum"  " From :help xterm-true-color (but st)
set termguicolors                       " Enable support for truecolor (24bit)
syntax on

" vim:ft=vim
